{
	"manifest_version": 3,
	"name": "Instagram Comment Hider",
	"description": "Disable comments on Instagram to reduce brainrot by up to 80%!",
	"version": "1.0",
	"background": {
	"service_worker": "background.js"
	},
	"permissions": ["scripting", "activeTab"],
    "content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
		}
	]
}

